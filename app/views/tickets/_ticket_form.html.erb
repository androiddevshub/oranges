<%= form_with(model: [@order, ticket], local: true) do |form| %>
<p>
<%= form.text_field :kg, :placeholder => "kg"%>
<%= form.text_field :fruit, :placeholder => "type of fruit" %>
<%= form.text_field :ppfruit, :placeholder => "price p kg" %>
<%= form.text_field :tprice, :placeholder => "total price, dont touch this!" %>
<p/>
<p>
    <%= form.submit %>
<p/>
<% end %>